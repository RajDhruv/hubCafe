<%case from%>
<%when 'new_request_log'%>
$('#invitation_link_<%=@cafe.id%>').html('Request Sent').css('color','orange').attr('onclick','');
<%when 'view_requests'%>
$('#action_zone').html("<%= escape_javascript(render :partial=>'invitations/view_requests.html.erb' )%>");
<%when 'take_action'%>
<%if @success==true%>
	$('#invitation_action_<%=@invitation.id%>').html("");
<%end%>
<%end%>
$("#message_pop_up").html("<%= escape_javascript(render :partial=>'shared/noty_message_popup.html.erb' )%>");