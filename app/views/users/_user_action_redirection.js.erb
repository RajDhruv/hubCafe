<%case from%>
<%when 'register'%>
$("#form_content").html("<%= escape_javascript(render :partial=>'users/register.html.erb' )%>")
<%when 'userCreate'%>
<%if @success%>
$("#form_content").html("<%= escape_javascript(render :partial=>'users/fresh_login.html.erb' )%>")
<%end%>
<%when 'login'%>
	<%if session[:current_user].nil?%>
		$("#form_content").html("<%= escape_javascript(render :partial=>'users/fresh_login.html.erb' )%>")
	<%else%>
		$("#form_content").html("<%= escape_javascript(render :partial=>'users/re_login.html.erb' )%>")
	<%end%>
<%when 'validateLogin'%>
	<%if @success%>
		$("#form_content").html("<%= escape_javascript(render :partial=>'users/home_page.html.erb')%>")
		$('#navbar_links_listing').html("<%= escape_javascript(render :partial=>'users/active_users_navlinks.html.erb')%>")
	<%end%>
<%when 'logout'%>
	$("body").html("<%= escape_javascript(render :template=>'users/welcome.html.erb' )%>");
<%when 'my_profile'%>
	$("#action_zone").html("<%= escape_javascript(render :partial=>'users/my_profile.html.erb' )%>");
<%end%>
$("#message_pop_up").html("<%= escape_javascript(render :partial=>'shared/noty_message_popup.html.erb' )%>")